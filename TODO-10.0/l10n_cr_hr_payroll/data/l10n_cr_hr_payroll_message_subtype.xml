<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <record id="mt_payslip_draft" model="mail.message.subtype">
            <field name="name">Set to draft</field>
            <field name="res_model">hr.payslip</field>
            <field name="default" eval="True"/>
            <field name="description">Payslip set to draft</field>
        </record>
        
        <record id="mt_payslip_verify" model="mail.message.subtype">
            <field name="name">Validated</field>
            <field name="res_model">hr.payslip</field>
            <field name="default" eval="True"/>
            <field name="description">Payslip validated</field>
        </record>
        
        <record id="mt_payslip_done" model="mail.message.subtype">
            <field name="name">Finished</field>
            <field name="res_model">hr.payslip</field>
            <field name="default" eval="True"/>
            <field name="description">Payslip successfully finished</field>
        </record>
        
        <record id="mt_payslip_cancel" model="mail.message.subtype">
            <field name="name">Cancelled</field>
            <field name="res_model">hr.payslip</field>
            <field name="default" eval="True"/>
            <field name="description">Payslip cancelled</field>
        </record>
  </data>
</openerp>
